CHIP 32Bit {
    IN in[32], load;
    OUT out[32];

    PARTS:
    8Bit(in=in[0..7],load=load,out=out[0..7]);
    8Bit(in=in[8..15],load=load,out=out[8..15]);
    8Bit(in=in[16..23],load=load,out=out[16..23]);
    8Bit(in=in[24..31],load=load,out=out[24..31]);
    }
